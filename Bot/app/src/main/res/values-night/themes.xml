<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Bot" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#98939F</item>
        <item name="colorPrimaryVariant">#9E95AC</item>
        <item name="colorOnPrimary">#98939F</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">#98939F</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="ThemeOverlay.Bot.FullscreenContainer" parent="">
        <item name="fullscreenBackgroundColor">#83A3B3</item>
        <item name="fullscreenTextColor">@color/light_blue_A400</item>
    </style>
</resources>